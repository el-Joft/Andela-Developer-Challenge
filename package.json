{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "eslint src/main/**/*.js",
    "lint:fix": "eslint src/main/**/*.js --fix",
    "clean": "rimraf build/",
    "build-babel": "babel src/main -d build/src/main",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./bin/server",
    "test": "cross-env NODE_ENV=test nyc mocha \"src/test/**/*.js\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chai": "^3.5.0",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "has": "^1.0.3",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "morgan": "~1.9.0",
    "randomstring": "^1.1.5",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-airbnb": "^2.5.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
